@model WebApplication1.Models.Course
@{
    ViewData["Title"] = "Grades";
}
<h1>@Html.DisplayNameFor(model => model.CourseTitle) @Html.DisplayFor(model => model.CourseTitle)</h1>
<table class="table">
    <thead>
        <tr>
            <th>
                AM
            </th>
            <th>
                Grade
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.CourseHasStudents)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.RegistrationNumber)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.GradeCourseStudent)
                </td>
                <td>
                    <a asp-action="Grading" asp-route-id="@item.RegistrationNumber,@item.IdCourse">Add Grade</a>

                </td>
            </tr>
        }
    </tbody>
</table>